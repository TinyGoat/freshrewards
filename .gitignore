# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore the database yml configuration.
/config/database.yml

# Ignore all logfiles and tempfiles.
/log
/tmp

#Ignore tags file
gems.tags
tags

#Ignore the Guardfile so that everyone can customize the way that they
#wish guard to function
Guardfile

#Ignore Zeus configuration files
zeus.json
custom_plan.rb

# Ignore the PID for the vagrant since its unique to each machine
/.vagrant

# Ignore OSX image caching data
*.DS_Store

